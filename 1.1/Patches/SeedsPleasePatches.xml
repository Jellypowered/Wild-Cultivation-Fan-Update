<Patch>

	<!-- Corn -->

	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="PatchOperationAdd">
				<xpath>Defs/SeedsPlease.SeedDef[defName = "SeedCorn"]/sources</xpath>
				<value>
					<li>PlantWildCorn</li>
				</value>
			</li>
		</operations>
	</Operation>

