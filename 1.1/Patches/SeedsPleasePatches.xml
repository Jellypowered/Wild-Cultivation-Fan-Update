<Patch>

	<!-- Corn -->

	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="PatchOperationAdd">
				<xpath>Defs/SeedsPlease.SeedDef[defName = "SeedCorn"]</xpath>
				<value>
					<sources><li>PlantWildCorn</li></sources>
				</value>
