<Patch>

	<!-- Corn -->

	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="PatchOperationAdd">
				<xpath>Defs/SeedsPlease.SeedDef[defName = "Seed_Corn"]</xpath>
				<value>
					<sources><li>PlantWildCorn</li></sources>
				</value>
